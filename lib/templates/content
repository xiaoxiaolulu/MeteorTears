
    @unittest.skipIf({}, '条件为True ，用例跳过')
    @wraps.cases_runner
    @wraps.result_assert
    def {}(self, *args, **kwargs):
        """{}"""
        response = kwargs.get('response')
        self.assertEqual(kwargs.get('expect_assert_value'), kwargs.get('kwassert_value'))
